mixin s-partners()
	// start s-partners
	+b.SECTION.s-partners.section#s-partners&attributes(attributes)
		.side-wrapper
			.container
				h2 С нами работают
				+e.slider--js
					mixin partner-i(i)
						+e.wpap
							+e.A.item(href='#')
								img.res-i(src=`img/@2x/partner${i}.png` alt='')
					+partner-i('1')
					+partner-i('2')
					+partner-i('3')
					+partner-i('4')
					+partner-i('5')
					+partner-i('6')
					+partner-i('1')
					+partner-i('2')
					+partner-i('3')
					+partner-i('4')
					+partner-i('5')
					+partner-i('6')
				+e.slider-dots
	// end s-partners